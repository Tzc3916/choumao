coroutine.wrap(function()
    while task.wait(0.05) do
        local t = tick() * 1.5
        Title.TextColor3 = Color3.new(
            0.7 + 0.3 * math.sin(t),
            0.7 + 0.3 * math.sin(t + 2),
            0.7 + 0.3 * math.sin(t + 4)
        )
    end
end)()

